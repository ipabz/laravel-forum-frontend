    "keywords": ["laravel", "laravel-4", "laravel-5", "forum", "discuss", "frontend"],
    "license": "MIT",
    "authors": [
        {
            "name": "Riari",
            "email": "riari@teamteatime.net"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "riari/laravel-forum": "~4.2"
    },
    "autoload": {
        "psr-4": {
            "Riari\\Forum\\Frontend\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Riari\\Forum\\Frontend\\ForumFrontendServiceProvider"
            ]
        }
    }
}
